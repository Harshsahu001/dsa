class Solution {
    public boolean isAnagram(String s, String t) {
         char temp1[] = s.toCharArray();
         char temp2[] = t.toCharArray();
         Arrays.sort(temp1);
         Arrays.sort(temp2);

        if(s.length()!=t.length())
        {
            return false;
        }
        else{
            for(int i=0;i<s.length();i++)
            {
                if(temp1[i]!=temp2[i])
                {
                    return false;
                }

        }
    }
    return true;
    }
}
